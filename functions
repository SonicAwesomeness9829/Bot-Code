import random

def gen_pass(pass_length):
    elements = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
    password = ""
    for i in range(pass_length):
        password += random.choice(elements)
    return password

def gen_emodji():
    emoji = ["ğŸ˜€", "ğŸ™‚", "ğŸ˜†", "ğŸ¤£", "ğŸ˜‚", "ğŸ˜Š", "ğŸ¥¹", "ğŸ˜…", "ğŸ˜‡", "ğŸ¥°", "ğŸ˜", "ğŸ¤©", "ğŸ˜˜", "ğŸ˜œ", "ğŸ¤ª", "ğŸ˜", "ğŸ˜‹"]
    return random.choice(emoji)

def flip_coin():
    return random.choice(["HEADS", "TAILS"])

def check_cool(name):
    is_cool = random.choice([True, False])
    if is_cool:
        return f"Yes, {name} is cool! ğŸ˜"
    else:
        return f"No, {name} is not cool. ğŸ˜¬"

def roll_dice():
    return str(random.randint(1, 6))

